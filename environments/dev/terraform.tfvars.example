# Example terraform.tfvars for dev environment
# Rename to terraform.tfvars and update with your values

# Core Infrastructure 
resource_group_name = "aks-jenkins-dev-rg"
location            = "eastus"

# Custom Tags
common_tags = {
  Environment = "Development"
  Project     = "AKS-Jenkins-GitHub"
  ManagedBy   = "Terraform"
  Owner       = "YourTeamName"
}

# Network Configuration
vnet_name          = "aks-dev-vnet"
vnet_address_space = ["10.0.0.0/16"]

# AKS Configuration
cluster_name        = "aks-jenkins-dev-cluster"
kubernetes_version  = "1.27.3"
node_pool_vm_size   = "Standard_D2s_v3"
node_pool_count     = 2
node_pool_max_count = 5
node_pool_min_count = 1

# Database Configuration
postgres_server_name   = "aks-jenkins-dev-postgres"
postgres_sku_name      = "GP_Gen5_2"
postgres_version       = "11"
postgres_storage_mb    = 5120
postgres_db_name       = "appdb"
postgres_admin_user    = "postgresadmin"
postgres_admin_password = "YourSecurePasswordHere" # Change this!

# CI/CD Configuration
acr_name           = "aksjenkinsdevacr"
jenkins_vm_name    = "jenkins-dev-vm"
jenkins_vm_size    = "Standard_D2s_v3"
jenkins_admin_user = "jenkins"
jenkins_ssh_key    = "ssh-rsa  dummy@example.com"

# Security Configuration
key_vault_name = "aks-jenkins-dev-kv"

# Monitoring Configuration
log_analytics_workspace_name = "aks-jenkins-dev-analytics"